static void Main(String[] args)
{
	Console.Write("***** FIZZBUZZ *****\n");
	Console.Write("Input a number: ");
	int num = console.readLine();
	
	fizzbuzz(num);
}

public static void fizzbuzz(int x)
	{
		if((x % 5 == 0) && (x % 3 == 0))
			Console.Write("FizzBuzz");
		else if((x % 3) == 0)
			Console.Write("Fizz");
		else if((x % 5) == 0)
			Console.Write("Buzz");
	}